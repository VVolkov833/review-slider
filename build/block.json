{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "vv/review-slider",
  "title": "Gutenberg Review Slider",
  "version": "1.0.0",
  "textdomain": "vv833",
  "category": "widgets",
  "example": {},
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "fieldsets": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "default": ""
          },
          "content": {
            "type": "string",
            "default": ""
          }
        }
      }
    },
    "items": {
      "type": "number",
      "default": 2
    },
    "bg_color": {
      "type": "string",
      "default": "#abcdef"
    },
    "text_color": {
      "type": "string",
      "default": "#ffffff"
    }
  },
  "style": [
    "file:./style-index.css",
    "file:./index.css"
  ],
  "script": "file:./index.js",
  "editorStyle": "file:./index.css",
  "keywords": []
}