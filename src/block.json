{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vv/review-slider",
	"version": "0.1.0",
	"title": "Gutenberg Review Slider",
	"category": "widgets",
	"example": {},
	"attributes": {
		"fieldsets": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"title": { "type": "string", "default": "" },
					"content": { "type": "string", "default": "" }
				}
			}
		},
		"items": {
			"type": "number",
			"default": 2
		},
		"bg_color": { 
			"type": "string", 
			"default": "#abcdef" 
		},
		"text_color": { 
			"type": "string", 
			"default": "#ffffff" 
		}
	},
	"textdomain": "vv",
	"style": [
		"file:./style-index.css",
		"file:./index.css"
	],
	"script": "file:./index.js",
	"editorStyle": "file:./index.css",
	"keywords": []
}
